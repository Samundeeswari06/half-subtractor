#! /usr/local/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x3c58a720 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x3c589a40 .scope module, "tb_half_subtractor" "tb_half_subtractor" 3 3;
 .timescale 0 0;
v0x3c5af820_0 .var "A", 0 0;
v0x3c5af8e0_0 .var "B", 0 0;
v0x3c5af9b0_0 .net "borrow", 0 0, L_0x3c5afdb0;  1 drivers
v0x3c5afab0_0 .net "diff", 0 0, L_0x3c5afb80;  1 drivers
S_0x3c59bf20 .scope module, "hs" "half_subtractor" 3 6, 4 2 0, S_0x3c589a40;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "A";
    .port_info 1 /INPUT 1 "B";
    .port_info 2 /OUTPUT 1 "diff";
    .port_info 3 /OUTPUT 1 "borrow";
L_0x3c5afb80 .functor XOR 1, v0x3c5af820_0, v0x3c5af8e0_0, C4<0>, C4<0>;
L_0x3c5afdb0 .functor AND 1, L_0x3c5afce0, v0x3c5af8e0_0, C4<1>, C4<1>;
v0x3c59c530_0 .net "A", 0 0, v0x3c5af820_0;  1 drivers
v0x3c5af340_0 .net "B", 0 0, v0x3c5af8e0_0;  1 drivers
L_0x7f3c29129018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x3c5af400_0 .net *"_ivl_2", 0 0, L_0x7f3c29129018;  1 drivers
v0x3c5af4f0_0 .net *"_ivl_5", 0 0, L_0x3c5afce0;  1 drivers
v0x3c5af5d0_0 .net "borrow", 0 0, L_0x3c5afdb0;  alias, 1 drivers
v0x3c5af6e0_0 .net "diff", 0 0, L_0x3c5afb80;  alias, 1 drivers
L_0x3c5afce0 .arith/sub 1, L_0x7f3c29129018, v0x3c5af820_0;
    .scope S_0x3c589a40;
T_0 ;
    %vpi_call/w 3 13 "$monitor", "Time=%0d A=%b B=%b => diff=%b borrow=%b", $time, v0x3c5af820_0, v0x3c5af8e0_0, v0x3c5afab0_0, v0x3c5af9b0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x3c5af820_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x3c5af8e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x3c5af820_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x3c5af8e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x3c5af820_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x3c5af8e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x3c5af820_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x3c5af8e0_0, 0, 1;
    %delay 10, 0;
    %vpi_call/w 3 18 "$finish" {0 0 0};
    %end;
    .thread T_0;
